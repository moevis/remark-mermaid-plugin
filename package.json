{
  "name": "@moevis/remark-mermaid-plugin",
  "version": "1.0.3",
  "description": "remark mermaidjs plugin",
  "main": "./dist/cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build:cjs": "tsc --module commonjs --outDir dist/cjs/",
    "build:mjs": "tsc --module es6 --outDir dist/mjs/"
  },
  "keywords": [
    "remark",
    "mermaid"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mdast": "^3.0.0",
    "mermaid": "^9.1.5",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "@types/mermaid": "^8.2.9",
    "typescript": "^4.7.4"
  }
}
