{
  "name": "@moevis/remark-mermaid-plugin",
  "version": "1.0.4",
  "description": "remark mermaidjs plugin",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "remark",
    "mermaid"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mdast": "^3.0.0",
    "mermaid": "^9.1.5",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "@types/mermaid": "^8.2.9",
    "tsup": "^6.2.3",
    "typescript": "^4.7.4"
  }
}
